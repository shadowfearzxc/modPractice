<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.8.xsd">

    <include file="db/changelog/v1/changelog-1.2.2-create-tables.xml"/>
    <include file="db/changelog/v1/changelog-1.0.2-create-tables.xml"/>
    <include file="db/changelog/v1/changelog-1.3.2-create-tables.xml"/>
    <changeSet author="01.books-init" id="loadData">
        <loadData file="db/changelog/init/data.csv" tableName="books"/>
    </changeSet>
    <changeSet author="02.security-init" id="loadData-example">
        <loadData file="db/changelog/init/authorities.csv" tableName="authorities"/>
        <loadData file="db/changelog/init/roles.csv" tableName="roles"/>
        <loadData file="db/changelog/init/roles_authorities.csv" tableName="roles_authorities"/>
        <loadData file="db/changelog/init/users.csv" tableName="users"/>
    </changeSet>
    <changeSet author="03.library-init" id="loadData-library">
        <loadData file="db/changelog/init/data-library.csv" tableName="books_records"/>
    </changeSet>
</databaseChangeLog>
